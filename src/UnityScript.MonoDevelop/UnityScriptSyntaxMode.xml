	</Keywords>
	
	<Keywords color = "Keyword(Namespace)">
		<Word>package</Word>
		<Word>import</Word>
	</Keywords>
	
	<Keywords color = "Keyword(Property)">
		<Word>get</Word>
		<Word>set</Word>
		<Word>add</Word>
		<Word>remove</Word>
		<Word>value</Word>
	</Keywords>
	
